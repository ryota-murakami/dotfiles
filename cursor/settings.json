    "non-null-expression",
    "conditional-type",
    "mapped-type",
    "keyof",
    "nested-conditional-type",
    "infer",
    "never-keyword",
    "exclude-utility-type",
    "nonnullable-utility-type",
    "bigint",
    "record-utility-type",
    "type-alias-with-multiple-generics",
    "omit-utility-type",
    "interface-with-generics",
    "as-const-on-object",
    "extract-utility-type",
    "pick-utility-type"
