{
  "editor.inlineSuggest.enabled": true,
  "editor.scrollBeyondLastLine": true,
  "editor.scrollBeyondLastColumn": 10,
  "editor.language.brackets": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"]
  ],
  "editor.language.colorizedBracketPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"]
  ],
  "editor.guides.bracketPairs": "active",
  "editor.autoClosingDelete": "always",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.reportStyleChecksAsWarnings": false,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.tsc.autoDetect": "on",
  "typescript.tsdk": "./node_modules/typescript/lib",
  "typescript.tsserver.maxTsServerMemory": 4440,
  "typescript.preferences.renameMatchingJsxTags": true,
  "javascript.preferences.renameMatchingJsxTags": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.colorCustomizations": {
    "editorWhitespace.foreground": "#3f3f46",
    "statusBar.background": "#0a0a0a",
    "[Cursor Dark]": {},
    "terminal.background": "#00000000"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.fontSize": 15,
  "eslint.format.enable": true,
  "files.autoSave": "onFocusChange",
  "editor.renderWhitespace": "all",
  "editor.scrollbar.verticalScrollbarSize": 6,
  "editor.scrollbar.horizontalScrollbarSize": 6,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "foreground": "#bafdc16b"
        }
      }
    ]
  },
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {}
  },
  "extensions.ignoreRecommendations": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "css.lint.emptyRules": "ignore",
  "less.lint.emptyRules": "ignore",
  "scss.lint.emptyRules": "ignore",
  "css.lint.unknownAtRules": "error",
  "css.lint.unknownProperties": "error",
  "css.lint.unknownVendorSpecificProperties": "error",
  "css.lint.vendorPrefix": "ignore",
  "less.lint.unknownAtRules": "ignore",
  "less.lint.unknownProperties": "ignore",
  "eslint.lintTask.options": ". --ext .ts,.tsx,.js,jsx --fix",
  "editor.defaultColorDecorators": "auto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "git.autofetch": true,
  "editor.tabSize": 2,
  "css.lint.propertyIgnoredDueToDisplay": "ignore",
  "scss.lint.propertyIgnoredDueToDisplay": "ignore",
  "prettier.withNodeModules": true,
  "editor.codeActionsOnSave": {},
  "editor.indentSize": "tabSize",
  "workbench.list.fastScrollSensitivity": 1,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.locale": "en",
  "javascript.suggestionActions.enabled": false,
  "redhat.telemetry.enabled": true,
  "eslint.ignoreUntitled": true,
  "eslint.trace.server": "verbose",
  "git.openRepositoryInParentFolders": "never",
  "editor.guides.bracketPairsHorizontal": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "emmet.includeLanguages": {
    "postcss": "css"
  },
  "editor.inlineSuggest.showToolbar": "always",
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "editor.suggest.localityBonus": true,
  "editor.linkedEditing": true,
  "search.actionsPosition": "auto",
  "editor.multiCursorModifier": "alt",
  "editor.mouseWheelScrollSensitivity": 0.7,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.format.insertSpaceAfterConstructor": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "typescript.format.insertSpaceAfterTypeAssertion": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
  "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
  "typescript.format.semicolons": "remove",
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.tsserver.enableTracing": true,
  "js/ts.implicitProjectConfig.strictFunctionTypes": false,
  "js/ts.implicitProjectConfig.strictNullChecks": false,
  "git.suggestSmartCommit": true,
  "git.confirmSync": false,
  "eslint.lintTask.enable": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "json.schemas": [],
  "explorer.confirmDelete": false,
  "editor.accessibilitySupport": "off",
  "explorer.fileNesting.expand": false,
  "explorer.compactFolders": false,
  "workbench.settings.applyToAllProfiles": ["workbench.colorCustomizations"],
  "workbench.tree.indent": 14,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "/bin/bash", // or e.g. "/opt/homebrew/bin/bash"
      "args": ["-l"] // login shell so you get ~/.bash_profile
    },
    "zsh": { "path": "/bin/zsh", "args": ["-l"] },
    "fish": {
      "path": "/usr/local/bin/fish", // keep fish so you can still pick it
      "args": ["-l"]
    }
  },
  "terminal.integrated.defaultProfile.osx": "bash",
  "window.zoomLevel": 2,
  "launch": {
    "configurations": [],
    "compounds": []
  },
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "outline.collapseItems": "alwaysCollapse",
  "playwright.reuseBrowser": false,
  "playwright.showTrace": false,
  "cursor.cpp.disabledLanguages": [],
  "editor.formatOnPaste": true,
  "jest.jestCommandLine": "./node_modules/.bin/jest",
  "jest.runMode": "on-demand",
  "editor.gotoLocation.alternativeImplementationCommand": "editor.action.goToImplementation",
  "editor.gotoLocation.alternativeReferenceCommand": "editor.action.explainSymbol",
  "typescript.preferGoToSourceDefinition": true,
  "javascript.preferGoToSourceDefinition": true,
  "gitlens.plusFeatures.enabled": false,
  "gitlens.virtualRepositories.enabled": false,
  "gitlens.currentLine.pullRequests.enabled": false,
  "gitlens.menus": {
    "editor": {
      "blame": true,
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "editorGroup": {
      "blame": true,
      "compare": true
    },
    "editorGutter": {
      "compare": false,
      "remote": false,
      "share": false
    },
    "editorTab": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "clipboard": false,
      "compare": false,
      "history": false,
      "remote": false
    },
    "ghpr": {
      "worktree": true
    },
    "scm": {
      "graph": true
    },
    "scmRepositoryInline": {
      "graph": true,
      "stash": false
    },
    "scmRepository": {
      "authors": true,
      "generateCommitMessage": true,
      "graph": false
    },
    "scmGroupInline": {
      "stash": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true
    },
    "scmItemInline": {
      "stash": false
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true,
      "share": true,
      "stash": true
    }
  },
  "gitlens.gitCommands.search.showResultsInSideBar": false,
  "editor.folding": false,
  "gitlens.hovers.enabled": false,
  "files.exclude": {
    "**/.yarn": true
  },
  "debug.javascript.unmapMissingSources": true,
  "javascript.preferences.autoImportFileExcludePatterns": ["e2e"],
  "typescript.preferences.autoImportFileExcludePatterns": ["e2e"],
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"']([^\"']*).*?[\"']"],
    ["daisyui([^]*)", "daisyui=[\"']([^\"']*)"],
    ["clsx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"],
    ["classnames\\(([^)]*)\\)", "[\"'`]([^\"'`]*)[\"'`]"],
    ["(?:twMerge|twJoin)\\(([^;]*)[\\);]", "[`'\"`]([^'\"`;]*)[`'\"`]"]
  ],
  "surround.custom": {
    "Double Quote": {
      "label": "double quote",
      "disabled": false,
      "description": "\"...\"",
      "snippet": "\"${TM_SELECTED_TEXT/['\"](.*?)['\"]/$1/g}\"$0",
      "languageIds": ["html", "javascript", "typescript", "markdown"]
    },
    "Parenthesis": {
      "label": "()",
      "disabled": false,
      "description": "Just parenthesis",
      "snippet": "$($TM_SELECTED_TEXT)$0", // <-- snippet goes here.
      "languageIds": ["html", "javascript", "typescript", "markdown"]
    }
  },
  "editor.minimap.enabled": true,
  "gitlens.launchpad.indicator.enabled": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "cursor.general.enableShadowWorkspace": true,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "prettier.configPath": ".prettierrc",
  "typescript.tsserver.watchOptions": {
    "watchFile": "useFsEvents",
    "watchDirectory": "useFsEvents"
  },
  "typescript.preferences.importModuleSpecifierEnding": "minimal",
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "editor.inlayHints.fontSize": 15,
  "diffEditor.ignoreTrimWhitespace": true,
  "surround.with.arrowFunction": {
    "label": "arrowFunction",
    "description": "const $name = ($params) => { ... }",
    "disabled": false,
    "snippet": "const ${1:fnName} = (${2:params}) => {\n\t$TM_SELECTED_TEXT\n}$0"
  },
  "editor.dragAndDrop": false,
  "surround.with.templateLiteral": {
    "label": "Template Literal",
    "description": "`...` (Also replaces single and double quotes with backtick)",
    "disabled": false,
    "snippet": "`${TM_SELECTED_TEXT/['\"](.*?)['\"]/$1/g}`$0"
  },
  "surround.showOnlyUserDefinedSnippets": true,
  "cursor.cpp.enablePartialAccepts": true,
  "tailwindCSS.experimental.configFile": null,
  "typescript.inlayHints.parameterNames.enabled": "literals",
  "typescript.tsserver.web.typeAcquisition.enabled": true,
  "typescript.implementationsCodeLens.showOnInterfaceMethods": true,
  "editor.inlayHints.enabled": "off",
  "surround.with.element": {
    "label": "<element></element>",
    "description": "<element>...</element>",
    "disabled": false,
    "snippet": "<${1}$2>$TM_SELECTED_TEXT</$1>$0",
    "languageIds": [
      "html",
      "typescriptreact",
      "javascriptreact",
      "jsx",
      "markdown"
    ]
  },
  "security.promptForLocalFileProtocolHandling": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "cursor.composer.shouldAllowCustomModes": true,
  "cursor.composer.collapsePaneInputBoxPills": true,
  "cursor.general.globalCursorIgnoreList": [
    ".next/**/*",
    "node_modules/**/*",
    "dist/**/*",
    "build/**/*",
    "storybook-static",
    ".idea/**/*",
    "**/secret.json",
    "**/secrets.json",
    "**/*.key",
    "**/*.pem",
    "**/*.pfx",
    "**/*.p12",
    "**/*.crt",
    "**/*.cer",
    "**/id_rsa",
    "**/id_dsa",
    "**/.ssh/id_*"
  ],
  "window.titleBarStyle": "custom",
  "git.enableSmartCommit": true,
  "cursor.windowSwitcher.sidebarHoverCollapsed": true,
  "diffEditor.maxComputationTime": 0,
  "cursor.terminal.usePreviewBox": true,
  "extensions.gallery.serviceUrl": "https://marketplace.visualstudio.com/_apis/public/gallery",
  "extensions.allowed": "*",
  "cursor.composer.shouldAutoAcceptDiffs": false,
  "playwright.updateSnapshots": "all",
  "playwright.runGlobalSetupOnEachRun": false,
  "editor.experimental.preferTreeSitter.typescript": true,
  "typescript.preferences.useAliasesForRenames": false,
  "workbench.editor.enablePreview": false,
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "cursor.enable_git_worktrees_setting": true,
  "cursor.agent_layout_browser_beta_setting": true,
}
